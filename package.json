{
  "author": "Paul Serby <paul@serby.net>",
  "name": "uber-cache",
  "description": "Async in-memory that sets the interface for a number of uber-* caching engine: Memcached, Redis, MongoDB, LevelDB",
  "version": "2.0.0",
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "tags": [
    "cache",
    "memory",
    "ttl",
    "lru"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:serby/uber-cache"
  },
  "main": "./uber-cache.js",
  "scripts": {
    "lint": "./node_modules/.bin/jshint . --reporter=./node_modules/jshint-full-path/index.js",
    "pretest": "npm run-script lint",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -R spec -r should",
    "posttest": "./node_modules/.bin/istanbul check-coverage && rm -rf coverage",
    "prepublish": "npm test && npm prune"
  },
  "engines": {
    "node": ">=0.10"
  },
  "dependencies": {
    "async": "^0.9.0",
    "lodash.assign": "^2.4.1",
    "lru-cache": "^2.5.0",
    "sigmund": "^1.0.0"
  },
  "devDependencies": {
    "async": "^0.2.10",
    "istanbul": "^0.2.6",
    "jshint": "^2.4.4",
    "jshint-full-path": "^1.1.1",
    "mocha": "1",
    "should": "3"
  }
}
